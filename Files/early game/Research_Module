repeat = [Unlocks.Speed, Unlocks.Grass, Unlocks.Expand, Unlocks.Carrots, Unlocks.Watering, 	Unlocks.Fertilizer, Unlocks.Trees, Unlocks.Sunflowers, Unlocks.Mazes, Unlocks.Pumpkins, Unlocks.Polyculture ]
required_unautomatable_human_readable_only = [	Unlocks.Plant, Unlocks.Debug, Unlocks.Operators, Unlocks.Senses, Unlocks.Variables, Unlocks.Functions, 	Unlocks.Import, Unlocks.Lists ]

#use with unlocks(1) increase n with level of research for all available research
def unlocks(a):
	for repeat in num_unlocked(repeat) == 0:
		if num_items(repeat) < a:
			unlock(repeat)
	for onetime in num_unlocked(onetime) == 0:
		if num_items(onetime) < 1:
			unlock(onetime)
		return
